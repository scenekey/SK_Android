<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="@dimen/fab_size_mini"
    android:height="@dimen/fab_size_mini">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/white" />
            <size
                android:width="@dimen/_30sdp"
                android:height="@dimen/_30sdp" />
        </shape>
    </item>
    <item
        android:bottom="@dimen/floting_item_padding"
        android:drawable="@drawable/message"
        android:gravity="center"
        android:left="@dimen/floting_item_padding"
        android:right="@dimen/floting_item_padding"
        android:top="@dimen/floting_item_padding">
        <shape android:shape="rectangle">
            <size
                android:width="@dimen/_30sdp"
                android:height="@dimen/_30sdp" />
        </shape>

    </item>
</layer-list>
